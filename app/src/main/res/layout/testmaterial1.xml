<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout android:gravity="start"
        android:id="@+id/linearExammaterial1"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameGroup177"
            android:layout_height="@dimen/_87pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageRectangle2"
                android:layout_gravity="start"
                android:layout_height="@dimen/_87pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY" android:src="@drawable/img_rectangle2_3"/>
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup170"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageMenu"
                        android:layout_height="@dimen/_47pxh"
                        android:layout_width="@dimen/_42pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_menu"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMATERIALS"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{examMaterial1VM.examMaterial1Model.txtMATERIALS}"
                        style="@style/txtUbuntubold20" tools:text="@string/lbl_materials"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </FrameLayout>
        <ImageView android:id="@+id/imageImage2"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_264pxh"
            android:layout_marginEnd="@dimen/_59pxh"
            android:layout_marginStart="@dimen/_64pxh"
            android:layout_width="@dimen/_300pxh"
            android:scaleType="fitXY" imageRes="@{@drawable/img_image2}" tools:src="@drawable/img_image2"/>
        <LinearLayout android:id="@+id/linearGroup173"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_124pxv"
            android:layout_marginTop="@dimen/_43pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_2">
            <TextView android:ellipsize="end"
                android:id="@+id/txtExamMaterialV"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_27pxh"
                android:layout_marginStart="@dimen/_27pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{examMaterial1VM.examMaterial1Model.txtExamMaterialV}"
                style="@style/txtUbuntubold20_1" tools:text="@string/msg_exam_material_v"/>
            <HorizontalScrollView android:fillViewport="true"
                android:gravity="start"
                android:id="@+id/horizontalScrollGroup181"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_26pxv"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent" android:scrollbars="none">
                <LinearLayout android:id="@+id/linearGroup172"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <FrameLayout android:id="@+id/frameGroup179"
                        android:layout_height="@dimen/_144pxv"
                        android:layout_width="@dimen/_214pxh" style="@style/groupStylewhite_4">
                        <ImageView android:id="@+id/imageUpsc12"
                            android:layout_gravity="top|start"
                            android:layout_height="@dimen/_103pxh"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="@dimen/_107pxh"
                            android:scaleType="fitXY" circular="@{true}"
                            imageRes="@{@drawable/img_upsc12}" tools:src="@drawable/img_upsc12"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtUPSC"
                            android:layout_gravity="end"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_57pxv"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_57pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{examMaterial1VM.examMaterial1Model.txtUPSC}"
                            style="@style/txtUbuntubold20_1" tools:text="@string/lbl_upsc"/>
                    </FrameLayout>
                    <LinearLayout android:id="@+id/linearGroup175"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_52pxh"
                        android:layout_weight="0.63"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStylewhite_4">
                        <ImageView android:id="@+id/imageGate31"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_59pxh"
                            android:layout_marginEnd="@dimen/_33pxh"
                            android:layout_marginStart="@dimen/_33pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="@dimen/_147pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_gate31}" tools:src="@drawable/img_gate31"/>
                        <TextView android:ellipsize="end"
                            android:gravity="end"
                            android:id="@+id/txtGATE"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_20pxv"
                            android:layout_marginEnd="@dimen/_3pxh"
                            android:layout_marginStart="@dimen/_56pxh"
                            android:layout_marginTop="@dimen/_22pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{examMaterial1VM.examMaterial1Model.txtGATE}"
                            style="@style/txtUbuntubold20_1" tools:text="@string/lbl_gate"/>
                    </LinearLayout>
                </LinearLayout>
            </HorizontalScrollView>
        </LinearLayout>
    </LinearLayout>
</layout>
