<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearTestpattern1"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearGroup227"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameGroup231"
                android:layout_height="@dimen/_90pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageRectangle2"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_90pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY" android:src="@drawable/img_rectangle2"/>
                <LinearLayout android:id="@+id/linearGroup222"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearGroup240"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_weight="0.67"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMenu"
                            android:layout_height="@dimen/_47pxh"
                            android:layout_width="@dimen/_42pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_menu"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDETAILS"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_20pxv"
                            android:layout_marginStart="@dimen/_19pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_weight="0.66"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{testpattern1VM.testpattern1Model.txtDETAILS}"
                            style="@style/txtUbuntubold20" tools:text="@string/lbl_details"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageProfile31"
                        android:layout_height="@dimen/_63pxh"
                        android:layout_marginStart="@dimen/_169pxh"
                        android:layout_width="@dimen/_71pxh"
                        android:scaleType="fitXY" circular="@{true}"
                        imageRes="@{@drawable/img_profile31}" tools:src="@drawable/img_profile31"/>
                </LinearLayout>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameGroup233"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_141pxv"
                android:layout_marginEnd="@dimen/_44pxh"
                android:layout_marginStart="@dimen/_44pxh"
                android:layout_marginTop="@dimen/_49pxv" android:layout_width="@dimen/_300pxh">
                <ImageView android:id="@+id/imageMenu1"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_47pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_113pxh"
                    android:layout_marginStart="@dimen/_113pxh"
                    android:layout_width="@dimen/_42pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_menu"/>
                <ImageView android:id="@+id/imageGate31"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_131pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_gate33}" tools:src="@drawable/img_gate33"/>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearGroup223"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_82pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameGroup235"
                    android:layout_height="@dimen/_60pxv"
                    android:layout_marginStart="@dimen/_36pxh" android:layout_width="@dimen/_162pxh">
                    <ImageView android:id="@+id/imageRectangle22"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_60pxh"
                        android:layout_width="@dimen/_162pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_rectangle22_1"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDescription"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{testpattern1VM.testpattern1Model.txtDescription}"
                        style="@style/txtUbuntubold20" tools:text="@string/lbl_description"/>
                </FrameLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnTestPattern"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_42pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_14pxh"
                    android:paddingStart="@dimen/_14pxh"
                    android:paddingTop="@dimen/_16pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_test_pattern"
                    android:textAllCaps="false"
                    style="@style/btnSolidRoundedOutline" tools:text="@string/lbl_test_pattern"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAboutTheExam"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_53pxh"
                android:layout_marginStart="@dimen/_53pxh"
                android:layout_marginTop="@dimen/_49pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{testpattern1VM.testpattern1Model.txtAboutTheExam}"
                style="@style/txtUbuntubold20_1" tools:text="@string/lbl_about_the_exam"/>
            <FrameLayout android:id="@+id/frameGroup237"
                android:layout_gravity="end"
                android:layout_height="@dimen/_185pxv"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_356pxh">
                <FrameLayout android:id="@+id/frameGroup239"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_185pxv" android:layout_width="@dimen/_356pxh">
                    <TextView android:id="@+id/txtGATEGraduate"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:text="@{testpattern1VM.testpattern1Model.txtGATEGraduate}"
                        style="@style/txtUbuntubold15_1" tools:text="@string/msg_gate_graduate"/>
                    <ImageView android:id="@+id/imageRectangle242"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_44pxh"
                        android:layout_marginEnd="@dimen/_58pxh"
                        android:layout_marginStart="@dimen/_58pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_200pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_rectangle242_2"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAPPLY"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_107pxh"
                    android:layout_marginStart="@dimen/_107pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{testpattern1VM.testpattern1Model.txtAPPLY}"
                    style="@style/txtUbuntubold20" tools:text="@string/lbl_apply"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
